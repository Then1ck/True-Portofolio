package mainPanel;

import java.awt.AWTException;
import java.awt.Robot;

import javax.swing.JFrame;

import controls.MouseMovements;

public class SimulatedMouse {
	private MouseMovements MouseM;
	private int sMouseX, sMouseY;
	private Robot resetMouse;
	private JFrame mainFrame;

	public SimulatedMouse(MouseMovements mouseM, JFrame _mainFrame) {
		super();
		this.MouseM = mouseM;
		this.mainFrame = _mainFrame;
		try {
			this.resetMouse = new Robot();
		} catch (AWTException e) {
			e.printStackTrace();
		}
	}
	
	public void Reset() {
		
	}
}
